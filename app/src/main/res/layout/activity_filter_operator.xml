<?xml version="1.0" encoding="utf-8"?>
<ScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".rxjava.FilterOperatorActivity">

    <LinearLayout android:layout_width="match_parent"
                  android:layout_height="match_parent"
                  android:orientation="vertical">

        <android.support.v7.widget.AppCompatButton
        style="@style/itemStyle"
        android:text="debounce"
        android:onClick="debounce"
        />

        <android.support.v7.widget.AppCompatButton
        style="@style/itemStyle"
        android:text="distinct"
        android:onClick="distinct"
        />


        <android.support.v7.widget.AppCompatButton
        style="@style/itemStyle"
        android:text="distinctUntilChange"
        android:onClick="distinctUntilChange"
        />

        <android.support.v7.widget.AppCompatButton
        style="@style/itemStyle"
        android:text="elementAt"
        android:onClick="elementAt"
        />

        <android.support.v7.widget.AppCompatButton
                style="@style/itemStyle"
                android:text="filter"
                android:onClick="filter"
        />


        <android.support.v7.widget.AppCompatButton
                style="@style/itemStyle"
                android:text="first"
                android:onClick="first"
        />



        <android.support.v7.widget.AppCompatButton
                style="@style/itemStyle"
                android:text="firstElement"
                android:onClick="firstElement"
        />




        <android.support.v7.widget.AppCompatButton
                style="@style/itemStyle"
                android:text="ignoreElement"
                android:onClick="ignoreElement"
        />


        <android.support.v7.widget.AppCompatButton
                style="@style/itemStyle"
                android:text="last"
                android:onClick="last"
        />



        <android.support.v7.widget.AppCompatButton
                style="@style/itemStyle"
                android:text="ofType"
                android:onClick="ofType"
        />



        <android.support.v7.widget.AppCompatButton
                style="@style/itemStyle"
                android:text="simple"
                android:onClick="simple"
        />


        <android.support.v7.widget.AppCompatButton
                style="@style/itemStyle"
                android:text="skip"
                android:onClick="skip"
        />


        <android.support.v7.widget.AppCompatButton
                style="@style/itemStyle"
                android:text="take"
                android:onClick="take"
        />



        <android.support.v7.widget.AppCompatButton
                style="@style/itemStyle"
                android:text="timeout"
                android:onClick="timeout"
        />
    </LinearLayout>

</ScrollView>